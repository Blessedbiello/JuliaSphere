{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "JuliaOS Testing TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "types": ["jest", "node"],
    "jsx": "react-jsx",
    "noEmit": true,
    "allowJs": true,
    "checkJs": false
  },
  "include": ["src/**/*", "test/**/*", "**/*.test.ts", "**/*.spec.ts", "jest.setup.ts", "jest.config.ts"],
  "exclude": ["node_modules", "dist"]
}
