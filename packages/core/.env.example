# Environment
NODE_ENV=development

# Julia Configuration
JULIA_PATH=/path/to/julia
JULIA_PROJECT=/path/to/julia/project
JULIA_DEPOT_PATH=/path/to/julia/depot

# Bridge Configuration
JULIA_BRIDGE_PORT=8080
JULIA_BRIDGE_DEBUG=false
JULIA_BRIDGE_TIMEOUT=30000
JULIA_BRIDGE_MAX_RETRIES=3
JULIA_BRIDGE_RECONNECT_INTERVAL=5000
JULIA_BRIDGE_HEARTBEAT_INTERVAL=30000
JULIA_BRIDGE_HEARTBEAT_TIMEOUT=10000
JULIA_BRIDGE_MAX_QUEUE_SIZE=1000
JULIA_BRIDGE_BACKOFF_MULTIPLIER=1.5

# Storage Configuration
STORAGE_BACKUP_INTERVAL=3600000
STORAGE_MAX_BACKUPS=5
STORAGE_COMPRESSION=true

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/development.log 