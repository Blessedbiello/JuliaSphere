{
    "environment": "test",
    "api_keys": {
        "ethereum": "test-eth-key",
        "solana": "test-sol-key",
        "market_data": "test-market-key"
    },
    "bridge": {
        "chains": {
            "ethereum": {
                "rpc_url": "http://localhost:8545",
                "chain_id": 1,
                "gas_limit": 21000,
                "gas_price": 50.0,
                "confirmations": 1,
                "timeout": 30
            },
            "solana": {
                "rpc_url": "http://localhost:8899",
                "chain_id": 1,
                "gas_limit": 100000,
                "gas_price": 0.00001,
                "confirmations": 1,
                "timeout": 30
            }
        },
        "max_retries": 100,
        "retry_delay": 3,
        "timeout": 30,
        "gas_limit": 12
    },
    "agents": {
        "test_agent": {
            "name": "test_agent",
            "type": "Arbitrage Agent",
            "strategy": "Mean Reversion",
            "chains": ["ethereum"],
            "risk_params": {
                "max_position_size": 0.1,
                "min_profit_threshold": 0.01,
                "max_gas_price": 50.0,
                "confidence_threshold": 0.9
            }
        }
    },
    "swarms": {
        "test_swarm": {
            "name": "test_swarm",
            "population_size": 10,
            "algorithm": "pso",
            "trading_pairs": ["ETH/USDC"],
            "parameters": {
                "inertia_weight": 0.7,
                "cognitive_coef": 1.5,
                "social_coef": 1.5,
                "population_size": 100,
                "max_iterations": 1000
            }
        }
    },
    "dashboard": {
        "host": "127.0.0.1",
        "port": 8000,
        "update_interval": 5,
        "max_history": 1000,
        "alerts": {
            "max_drawdown": 0.1,
            "min_win_rate": 0.5,
            "min_sharpe": 1.0
        }
    },
    "logging": {
        "level": "DEBUG",
        "file": "test.log",
        "max_size": 1000000,
        "backup_count": 2
    },
    "backup": {
        "enabled": true,
        "interval": 60,
        "max_backups": 5,
        "path": "test_backups"
    }
} 